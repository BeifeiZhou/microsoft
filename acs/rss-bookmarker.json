{
  "id": "rss-bookmarker",
  "mem": 512,
  "cpus": 1.0,
  "instances": 2,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "cvugrinec/rss_bookmarker:1.5",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 8080, "hostPort": 0, "servicePort": 10001, "protocol": "tcp" }
      ]
    }
  },
  "healthChecks": [
    {
      "protocol": "HTTP",
      "portIndex": 0,
      "path": "/rss-bookmarker/"
    }
  ],
  "labels":{
    "HAPROXY_GROUP":"external"
  }
}
