{
  "id": "twitterapp",
  "mem": 512,
  "cpus": 1.0,
  "instances": 2,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "cvugrinec/twitterapp:1.2",
      "network": "BRIDGE",
      "portMappings": [{ "containerPort": 8080, "hostPort": 0, "servicePort": 10002, "protocol": "tcp" } ]
    }
  },
  "healthChecks": [
    {
      "protocol": "HTTP",
      "portIndex": 0,
      "path": "/status"
    }
  ],
  "labels":{
    "HAPROXY_GROUP":"external",
    "HAPROXY_0_VHOST":"acsdemo1-agents.northeurope.cloudapp.azure.com",
    "HAPROXY_0_MODE":"http"
  }
}
